page {
  width: 100%;
}
.cates {
  width: 100%;
}
.cates .cates_content {
  height: calc(100vh - 90rpx);
  display: flex;
}
.cates .cates_content .cates_left {
  flex: 2;
}
.cates .cates_content .cates_left .left_menu {
  display: flex;
  height: 80rpx;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
}
.cates .cates_content .cates_left .active {
  color: var(--themeColor);
  border-left: 5rpx solid currentColor;
}
.cates .cates_content .cates_right {
  flex: 5;
}
.cates .cates_content .cates_right .goods_group {
  overflow: hidden;
}
.cates .cates_content .cates_right .goods_group .goods_title {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cates .cates_content .cates_right .goods_group .goods_title .delimiter {
  color: #ccc;
  padding: 0 10rpx;
}
.cates .cates_content .cates_right .goods_group .goods_list {
  display: flex;
  flex-wrap: wrap;
}
.cates .cates_content .cates_right .goods_group .goods_list navigator {
  display: block;
  width: 33.33%;
  text-align: center;
}
.cates .cates_content .cates_right .goods_group .goods_list navigator image {
  width: 50%;
}
